<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="339dp"
        android:layout_height="249dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.81">

        <TextView
            android:id="@+id/grid1_1"
            android:layout_width="86dp"
            android:layout_height="62dp"
            app:layout_column="1"
            app:layout_row="1" />

        <TextView
            android:id="@+id/grid1_2"
            android:layout_width="86dp"
            android:layout_height="62dp"
            app:layout_column="1"
            app:layout_row="2" />

        <TextView
            android:id="@+id/grid1_3"
            android:layout_width="86dp"
            android:layout_height="62dp"
            app:layout_column="1"
            app:layout_row="3" />

        <TextView
            android:id="@+id/grid0_1"
            android:layout_width="89dp"
            android:layout_height="62dp"
            app:layout_column="0"
            app:layout_row="1" />

        <TextView
            android:id="@+id/grid0_2"
            android:layout_width="89dp"
            android:layout_height="62dp"
            app:layout_column="0"
            app:layout_row="2" />

        <TextView
            android:id="@+id/grid0_3"
            android:layout_width="89dp"
            android:layout_height="62dp"
            app:layout_column="0"
            app:layout_row="3" />

        <TextView
            android:id="@+id/grid2_0"
            android:layout_width="84dp"
            android:layout_height="61dp"
            app:layout_column="2"
            app:layout_row="0" />

        <TextView
            android:id="@+id/grid2_1"
            android:layout_width="84dp"
            android:layout_height="62dp"
            app:layout_column="2"
            app:layout_row="1" />

        <TextView
            android:id="@+id/grid2_2"
            android:layout_width="84dp"
            android:layout_height="62dp"
            app:layout_column="2"
            app:layout_row="2" />

        <TextView
            android:id="@+id/grid2_3"
            android:layout_width="84dp"
            android:layout_height="62dp"
            app:layout_column="2"
            app:layout_row="3" />

        <TextView
            android:id="@+id/grid3_0"
            android:layout_width="83dp"
            android:layout_height="62dp"
            app:layout_column="3"
            app:layout_row="0" />

        <TextView
            android:id="@+id/grid3_1"
            android:layout_width="83dp"
            android:layout_height="62dp"
            app:layout_column="3"
            app:layout_row="1" />

        <TextView
            android:id="@+id/grid3_2"
            android:layout_width="83dp"
            android:layout_height="62dp"
            app:layout_column="3"
            app:layout_row="2" />

        <TextView
            android:id="@+id/grid3_3"
            android:layout_width="83dp"
            android:layout_height="62dp"
            app:layout_column="3"
            app:layout_row="3" />

        <TextView
            android:id="@+id/grid1_0"
            android:layout_width="86dp"
            android:layout_height="62dp"
            app:layout_column="1"
            app:layout_row="0" />

        <TextView
            android:id="@+id/grid0_0"
            android:layout_width="89dp"
            android:layout_height="62dp"
            android:textAlignment="center"
            app:layout_column="0"
            app:layout_row="0" />
    </androidx.gridlayout.widget.GridLayout>

    <Button
        android:id="@+id/Main_Screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:text="@string/leave"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.378" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="222dp"
        android:layout_height="92dp"
        android:text="a"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.414" />

</androidx.constraintlayout.widget.ConstraintLayout>